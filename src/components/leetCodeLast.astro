---
import Card from "./Card/index.astro";
const response = await fetch("https://leetcode-api-faisalshohag.vercel.app/egromero",
  {
    method: 'GET',
    headers: {
        'Content-Type':'application/json',
        'Authorization': 'Bearer '+ import.meta.env.gh_token,},
    });
const {recentSubmissions} = await response.json();
console.log(recentSubmissions);
const lastSubmission = recentSubmissions[0];
---
<Card colSpan="md:col-span-1" rowSpan="md:row-span-2" title="Last LeetCode Solve">
    <div class="text-white font-bold italic mt-2 text-sm">
        "{lastSubmission.title}"<br/>
        <p class="mt-2 not-italic">{new Date(lastSubmission.timestamp * 1000).toUTCString()}</p>
    </div>
</Card>